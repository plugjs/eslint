{
  "name": "@plugjs/eslint",
  "version": "0.1.1",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./eslint": {
      "require": {
        "types": "./dist/eslint.d.ts",
        "default": "./dist/eslint.cjs"
      },
      "import": {
        "types": "./dist/eslint.d.ts",
        "default": "./dist/eslint.mjs"
      }
    }
  },
  "scripts": {
    "build": "plug"
  },
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@plugjs/cov8": "^0.1.1",
    "@plugjs/eslint-plugin": "^0.1.1",
    "@plugjs/plug": "^0.1.2",
    "@plugjs/typescript": "^0.1.1",
    "@types/eslint": "^8.4.10",
    "@types/node": "<19",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "eslint": "^8.29.0"
  },
  "peerDependencies": {
    "@plugjs/plug": "^0.1.2"
  },
  "files": [
    "*.md",
    "dist/",
    "src/"
  ]
}
